package may21;
// fizz buzz
public class For02 {
	public static void main(String[] args) {
	
			int result = 0;
			for (int i = 0; i <= 25; i++) {
				if ( i % 3 == 3) {
					result += i;
					System.out.println("fizz");
					
				}else {
					if ( i % 5 == 5) {
						result += i;
						System.out.println("buzz");
					}
					
			}
	}
}
