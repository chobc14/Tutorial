package may21;

public class For03 {
	public static void main(String[] args) {
		for (int i = 0; i <11; i++) {
			if(i==5) {
				break; //5가 안 찍힘
			}
			System.out.println("2 X " + i + " = " + (2 * i));
		//	System.out.printf("2 X %d = %d\n", i , i * 2 );
			 // 상단의 문장만 실행		
		}
		
		//continue = 이하 실행구문은 무시하고 다시 반복문으로 돌림
		//break =  가장 가까운 조건문 탈피
		
		
		
	
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}
}
