package may18;
// 실수
	//float
	//double(기본)
public class DataType2 {

	public static void main(String[] args) {
		//데이터타입 변수명 = 값;
		//3.14
		double pi = 3.14;
		System.out.println(pi);

		int intNum = (int) pi;
		System.out.println(intNum);//3
		
		long longNum = (long) pi;
		System.out.println(longNum);//3
		
		pi = longNum;
		
	
	
	}
	
	

}
